						},
					"pxy-to-ly": {
						"description": "Get logic Y by physical X,Y co-ordinate.",
						"translated-name": "PXY2LY",
						"params": {
							"x0": { "name":"X", "desc":"The physical X."},
							"y1": { "name":"Y", "desc":"The physical Y."}
						}
